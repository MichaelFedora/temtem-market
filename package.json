{
  "name": "temtem-market",
  "version": "1.3.0",
  "description": "A Market for Temtem",
  "scripts": {
    "lint": "eslint . --ext .ts,.vue",
    "test": "node build/backend/temtem-market.js",
    "start": "node build-prod/backend/temtem-market.js",
    "clean-backend": "trash build/{backend,common/util,common/static-serve}/*",
    "clean-backend:prod": "trash build-prod/{backend,common/util,common/static-serve}/*",
    "tsc-backend": "tsc -p tsconfig.backend.json",
    "tsc-backend:prod": "tsc -p tsconfig.backend.json --outDir ./build-prod/backend",
    "cp-backend": "node scripts/copy-backend.js",
    "cp-backend:prod": "node scripts/copy-backend.js production",
    "build": "npm-run-all \"build:*\"",
    "build:backend": "npm-run-all clean-backend tsc-backend cp-backend",
    "build:frontend": "webpack",
    "build-prod": "npm-run-all \"build-prod:*\"",
    "build-prod:backend": "npm-run-all clean-backend:prod tsc-backend:prod cp-backend:prod",
    "build-prod:frontend": "webpack --mode=production"
  },
  "private": true,
  "devDependencies": {
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.9",
    "@types/fs-extra": "^9.0.6",
    "@types/lodash": "^4.14.167",
    "@types/luxon": "^1.25.0",
    "@types/node": "^14.14.19",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "autoprefixer": "^10.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "eslint": "^7.17.0",
    "eslint-plugin-vue": "^7.4.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.0.8",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.0",
    "mini-css-extract-plugin": "^1.3.3",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.2",
    "postcss-loader": "^4.1.0",
    "sass-loader": "^10.1.0",
    "terser-webpack-plugin": "^5.0.3",
    "trash-cli": "^4.0.0",
    "ts-loader": "^8.0.13",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.1.3",
    "vue-eslint-parser": "^7.3.0",
    "vue-loader": "^15.9.6",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^5.11.1",
    "webpack-cli": "^4.3.1"
  },
  "author": "MichaelFedora",
  "license": "See LICENSE in LICENSE.md",
  "dependencies": {
    "@log4js-node/log4js-api": "^1.0.2",
    "@mdi/font": "^5.8.55",
    "axios": "^0.21.1",
    "buefy": "^0.9.4",
    "bulma": "^0.9.1",
    "clipboard-copy": "^4.0.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "fs-extra": "^9.0.1",
    "helmet": "^4.3.1",
    "lodash": "^4.17.20",
    "log4js": "^6.3.0",
    "luxon": "^1.25.0",
    "pm2-intercom": "^1.0.0",
    "querystring": "^0.2.0",
    "rethinkdb-ts": "^2.4.5",
    "rxjs": "^6.6.3",
    "source-map-support": "^0.5.19",
    "uuid": "^8.3.2",
    "vee-validate": "^3.4.5",
    "vue": "^2.6.12",
    "vue-router": "^3.4.9"
  }
}
