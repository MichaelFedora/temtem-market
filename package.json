{
  "name": "temtem-market",
  "version": "1.0.0",
  "description": "A Market for Temtem",
  "scripts": {
    "lint": "eslint . --ext .ts,.vue",
    "test": "node build/backend/temtem-market.js",
    "start": "node build-prod/backend/temtem-market.js",
    "clean-backend": "trash build/{backend,common/util,common/static-serve}/*",
    "clean-backend:prod": "trash build-prod/{backend,common/util,common/static-serve}/*",
    "tsc-backend": "tsc -p tsconfig.backend.json",
    "tsc-backend:prod": "tsc -p tsconfig.backend.json --outDir ./build-prod",
    "cp-backend": "node scripts/copy-backend.js",
    "cp-backend:prod": "node scripts/copy-backend.js production",
    "build": "npm-run-all \"build:*\"",
    "build:backend": "npm-run-all clean-backend tsc-backend cp-backend",
    "build:frontend": "webpack --config webpack/frontend.config.js",
    "build-prod": "npm-run-all \"build-prod:*\"",
    "build-prod:backend": "npm-run-all clean-backend:prod tsc-backend:prod cp-backend:prod",
    "build-prod:frontend": "webpack --mode=production --config webpack/frontend.config.js"
  },
  "private": true,
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/fs-extra": "^8.0.1",
    "@types/helmet": "0.0.45",
    "@types/lodash": "^4.14.149",
    "@types/luxon": "^1.21.0",
    "@types/node": "^13.5.1",
    "@types/uuid": "^3.4.7",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "autoprefixer": "^9.7.4",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.0",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.1.2",
    "file-loader": "^5.0.2",
    "fork-ts-checker-webpack-plugin": "^4.0.3",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^4.0.0-beta.11",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.2",
    "terser-webpack-plugin": "^2.3.3",
    "trash-cli": "^3.0.0",
    "ts-loader": "^6.2.1",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.7.5",
    "vue-eslint-parser": "^7.0.0",
    "vue-loader": "^15.8.3",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "author": "MichaelFedora",
  "license": "See LICENSE in LICENSE.md",
  "dependencies": {
    "@log4js-node/log4js-api": "^1.0.2",
    "@mdi/font": "^4.8.95",
    "axios": "^0.19.2",
    "buefy": "^0.8.10",
    "bulma": "^0.8.0",
    "clipboard-copy": "^3.1.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "fs-extra": "^8.1.0",
    "helmet": "^3.21.2",
    "lodash": "^4.17.15",
    "log4js": "^6.1.0",
    "luxon": "^1.22.0",
    "pm2-intercom": "^1.0.0",
    "rethinkdb-ts": "^2.4.2",
    "rxjs": "^6.5.4",
    "source-map-support": "^0.5.16",
    "uuid": "^3.4.0",
    "vee-validate": "^3.2.2",
    "vue": "^2.6.11",
    "vue-router": "^3.1.5",
    "vuex": "^3.1.2",
    "vuex-persistedstate": "^2.7.0"
  }
}
